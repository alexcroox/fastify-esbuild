{
  "name": "fastify-esbuild",
  "version": "0.0.12",
  "scripts": {
    "prepare": "husky install",
    "dev": "nodemon",
    "build": "rm -rf build && esbuild `find src \\( -name '*.ts' \\)` --platform=node --outdir=build --bundle",
    "build:windows": "rm -rf build && node esbuild.js",
    "format": "prettier --write 'src/**/*.{js,ts,json,md}'",
    "lint": "prettier --check 'src/**/*.{js,ts,json,md}' && eslint --ignore-path .gitignore .",
    "cz": "cz",
    "release": "standard-version --no-verify"
  },
  "author": "David Peng",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@commitlint/cz-commitlint": "^17.0.0",
    "@types/node": "^17.0.34",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "commitizen": "^4.2.4",
    "esbuild": "^0.14.39",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "json-schema-to-ts": "^2.4.0",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@fastify/autoload": "^4.0.1",
    "@fastify/sensible": "^4.1.0",
    "@fastify/swagger": "^6.0.1",
    "fastify": "^3.29.0",
    "fastify-plugin": "^3.0.1",
    "pino-pretty": "^7.6.1"
  }
}
